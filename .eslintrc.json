{
	"extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"plugins": ["prettier"],
	"env": {
		"node": true
	},
	"rules": {
		"no-console": "off",
		"import/extensions": [
			"error",
			"ignorePackages",
			{
				"ts": "never"
			}
		],
		"prettier/prettier": "error"
	}
}
